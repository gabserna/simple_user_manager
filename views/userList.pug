html
    head
        title
        link(rel="stylesheet", href="style.css")
    body
        div(class="container")
            div(class="main")
                h1 Users list
                table
                    tr
                        th user
                        th name
                        th email
                        th age
                        th changes
                    each user in users
                        tr
                            td= user.username
                            td= user.name
                            td= user.email
                            td= user.age
                            td
                                a(href="/edit/" + userId) = "Edit"
                                a(href="/delete/" + userId) = "Delete"
            div(class="btn")
                a.btn(href="/newUser") +