html
    head
        title User List
        link(rel="stylesheet", href="/styles.css")
    body
        div(class="container")
            div(class="main")
                h1 Users list
                table
                    tr
                        th user
                        th name
                        th email
                        th age
                        th changes
                    each user in users
                        tr
                            td= user.username
                            td= user.name
                            td= user.email
                            td= user.age
                            td
                                button(type="button", onclick=`window.location.href='/editList/' + '${user.userId}'`) Edit
                                button(type="button", onclick=`window.location.href='/deleteUser/' + '${user.userId}'`) Delete
                button(type="button", onclick="window.location.href='/newUser'") Add user